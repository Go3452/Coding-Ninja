import java.util.Scanner;
public class Solution {

	public static void main(String[] args)  
	{
	 Scanner sc = new Scanner(System.in);
	 int t=sc.nextInt();
	 for(int k=0;k<t;k++)
	 {
	  int n=sc.nextInt();
	  for(int i=0;i<n;i++)
	  {
	   String s="";
	   for(int j=0;j<i;j++)
	    s+=" ";
	   for(int j=i;j<n;j++)
	    s+="*";
	   System.out.println(s);
	  }
	 }
	}
}
