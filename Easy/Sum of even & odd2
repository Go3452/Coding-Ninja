import java.util.Scanner;
class Solution {
	
	public static void main(String args[]) {
		Scanner sc = new Scanner(System.in);
		int n=sc.nextInt();
		int a=0,b=0;
		while(n>0)
		{
		 int num=n%10;
		 if(num%2==0)
		  a+=num;
		 else
          b+=num;
		  n/=10;
		}
		System.out.println(a + " "+b);
	}
}
