import java.util.ArrayList;
import java.util.List;
public class Solution{
    public static List< Integer > printDivisors(int n) {
        ArrayList<Integer> arr = new ArrayList<Integer>();
        arr.add(1);
        for(int i=2;i<=n/2;i++)
        {
         if(n%i==0)
          arr.add(i);
        }
        arr.add(n);
        return arr;
    }
}
