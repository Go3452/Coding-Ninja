import java.util.* ;
import java.io.*; 
import java.util.Scanner;

class Solution {
	
	public static void main(String args[]) {
		
		Scanner read = new Scanner(System.in);
		String name=read.nextLine();
		int age=read.nextInt();
		System.out.println("My name is " + name + " and my age is " + age + ".");
	}
}
